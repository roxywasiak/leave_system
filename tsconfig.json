{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strictNullChecks": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "types": ["express", "jest"],
    "strict": false,
  
  },
  
  "include": ["src/**/*"],
   "exclude": ["node_modules"]
}


